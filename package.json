{
  "name": "aa-server",
  "version": "1.1.0",
  "main": "src/index.js",
  "author": "svirins",
  "license": "MIT",
  "description": "AA meetings locator - backend",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server.ts",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "lint": "eslint src/**/**/*.ts tests/**/**/*.ts",
    "tsc": "tsc",
    "generate:nexus": "ts-node-dev --transpile-only src/schema",
    "generate:prisma": "prisma generate",
    "lint": "eslint src/**/**/*.ts tests/**/**/*.ts",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "migrate:down": "prisma migrate down 1 --experimental"
  },
  "dependencies": {
    "apollo-server": "^2.17.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^0.26.1",
    "nexus-plugin-prisma": "^0.18.2"
  },
  "devDependencies": {
    "@types/node": "^14.10.2",
    "eslint": "^7.9.0",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svirins/apollo-prisma-nexus-server.git"
  }
}
